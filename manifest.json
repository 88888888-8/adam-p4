{
  "name": "Markdown Here",
  "version": "1.0",
  "description": "Write your email in Markdown, then make it pretty.",
  "permissions": ["contextMenus"],
  "minimum_chrome_version": "6",
  "background_page": "background.html",
  "icons": {
    "16": "butterfly16.png",
    "32": "butterfly32.png",
    "48": "butterfly48.png",
    "128": "butterfly128.png",
    "512": "butterfly512.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.7.2.min.js","marked.js","jsHtmlToText.js","github.css.js","markdown-here-listener.js"]
    }
  ]
}

{
  "name": "Markdown Here",
  "version": "1.2.1",
  "description": "Write your email in Markdown, then make it pretty.",
  "homepage_url": "https://chrome.google.com/webstore/detail/bonncgjadmfcadjlopgmclakggdgpdnm",
  "permissions": ["contextMenus"],
  "minimum_chrome_version": "6",
  "background_page": "lib/background.html",
  "icons": {
    "16": "images/butterfly16.png",
    "32": "images/butterfly32.png",
    "48": "images/butterfly48.png",
    "128": "images/butterfly128.png",
    "512": "images/butterfly512.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["data/markdown-here-listener.js", "data/chrome-contentscript.js"]
    }
  ]
}

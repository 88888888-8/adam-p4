{
  "name": "Markdown Here",
  "version": "1.2.1",
  "description": "Write your email in Markdown, then make it pretty.",
  "homepage_url": "https://chrome.google.com/webstore/detail/bonncgjadmfcadjlopgmclakggdgpdnm",
  "permissions": ["contextMenus"],
  "minimum_chrome_version": "6",
  "background_page": "chrome/background.html",
  "icons": {
    "16": "common/images/butterfly16.png",
    "32": "common/images/butterfly32.png",
    "48": "common/images/butterfly48.png",
    "128": "common/images/butterfly128.png",
    "512": "common/images/butterfly512.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["common/markdown-here.js", "chrome/chrome-contentscript.js"]
    }
  ]
}
